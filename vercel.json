{
  "functions": {
    "api/collect-data.js": {
      "maxDuration": 30
    }
  },
  "crons": [
    {
      "path": "/api/collect-data",
      "schedule": "*/10 * * * *"
    }
  ],
  "env": {
    "WAQI_API_TOKEN": "@waqi-api-token",
    "DATABASE_URL": "@database-url",
    "SUPABASE_URL": "@supabase-url",
    "SUPABASE_ANON_KEY": "@supabase-anon-key"
  }
}